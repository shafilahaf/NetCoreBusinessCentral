                    @Html.DisplayFor(modelItem => item.SellToCustomerNo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SellToCustomerName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OrderDate)
                </td>
                <td>
                    <a asp-action="PrintReportSO" asp-controller="ReportSales" asp-route-id="@item.No">Print</a>
                </td>
            </tr>
    }
  </tbody>
  <tfoot>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.No)
        </th>
